node("docker") {
    docker.withRegistry('https://hub.docker.com/r/engisoftecstests/test/', 'engisoftecstests') {
    
        git url: "https://bitbucket.org/engisoftcloudservices/ips-helloworld", credentialsId: 'd.regordosa@engisoft.com'
    
        sh "git rev-parse HEAD > .git/commit-id"
        def commit_id = readFile('.git/commit-id').trim()
        println commit_id
    
        stage "build"
        def app = docker.build "engisoftecstests/test"
    
        stage "publish"
        app.push 'master'
        app.push "${commit_id}"
    }
}